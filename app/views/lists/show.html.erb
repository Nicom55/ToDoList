<div>
  <%= content_tag :p, @list.title %>
  <% @items.each do |item| %>
    <%= content_tag :p, item.title %>
  <% end %>
  <nav>
    <div>
      <%= link_to "Delete", list_path(@list), method: :delete, data: { confirm: 'Are you sure?' } %>
      <%= link_to "Edit", edit_list_path(@list) %> 
      <%= link_to "Home", root_path %>
    </div>
  </nav>
</div>